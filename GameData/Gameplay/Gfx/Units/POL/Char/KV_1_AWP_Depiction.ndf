Gfx_KV_1_AWP is TTimelyReceiverFactoryCarrier
(
    DepictionTemplate = TDepictionTemplate
    (
        RenderStyleProviderDescriptor = ~/DefaultRenderStyleProvider
        Selector = SpecificDepictionSelectorWithScale
        DepictionAlternatives = [
            DepictionDescriptor_LOD_High       ( MeshDescriptor = Modele_KV_1_AWP ),
            DepictionDescriptor_LOD_Mid        ( MeshDescriptor = Modele_KV_1_AWP_MID ),
            DepictionDescriptor_LOD_Low_NoTurret3( MeshDescriptor = Modele_KV_1_AWP_LOW ),
            DepictionDescriptor_LOD_WreckedHigh( MeshDescriptor = Modele_KV_1_AWP ),
            DepictionDescriptor_LOD_WreckedMid ( MeshDescriptor = Modele_KV_1_AWP_MID ),
            DepictionDescriptor_LOD_WreckedLow_NoTurret3( MeshDescriptor = Modele_KV_1_AWP_LOW ),
        ]

        Operators = Operators_KV_1_AWP
                  + DepictionOperator_Chassis_HeavyTank
                  + DepictionOperator_Propulsion_ContinuousTrack_HeavyTank
                  + [DepictionOperator_TurretRecoil(TurretNum='1')]

        Actions = Weapons_KV_1_AWP
                + DepictionAction_StressLevel_2
                + DepictionAction_MovementFX_Tracked

    )
)
