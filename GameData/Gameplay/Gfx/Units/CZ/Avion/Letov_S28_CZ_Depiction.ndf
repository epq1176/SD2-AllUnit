Gfx_Letov_S28_CZ is TTimelyReceiverFactoryCarrier
(
    DepictionTemplate = TDepictionTemplate
    (
        RenderStyleProviderDescriptor = ~/DefaultRenderStyleProvider
        Selector = SpecificAirplaneDepictionSelectorWithScale
        DepictionAlternatives = [
            DepictionDescriptor_LOD_High       ( MeshDescriptor = Modele_Letov_S28_CZ ),
            DepictionDescriptor_LOD_Mid        ( MeshDescriptor = Modele_Letov_S28_CZ_MID ),
            DepictionDescriptor_LOD_Low        ( MeshDescriptor = Modele_Letov_S28_CZ_LOW ),
            DepictionDescriptor_LOD_WreckedHigh( MeshDescriptor = Modele_Letov_S28_CZ ),
            DepictionDescriptor_LOD_WreckedMid ( MeshDescriptor = Modele_Letov_S28_CZ_MID ),
            DepictionDescriptor_LOD_WreckedLow ( MeshDescriptor = Modele_Letov_S28_CZ_LOW ),
        ]

         Operators = Operators_Letov_S28_CZ
                   + DepictionOperator_Airfoil_WingsAndStabilizersAirplane
                   + DepictionOperator_Propulsion_Single_Engine_Plane
                   + DepictionOperator_Airplane
                + DepictionOperator_Feedback_Degat_Level1
                   + DepictionOperator_Feedback_Degat_Level2



        Actions = //DepictionAction_MovementFX_SingleReactorAirplane
                Weapons_Letov_S28_CZ
                + DepictionAction_StressLevel_Avion_2
                + MAP [ (['FX_Helice_1'], Template_DepictionAction_Rotor( PaleLength = 380 PaleCount = 3 SousMobile = "Bloc_Moteur_1" ))]

        SubDepictionGenerators = SubGenerators_Letov_S28_CZ

    SubDepictions = [
        TSubDepiction(
            Anchor = "Landing Gear"
            // Anchor = 'chassis'
            Depiction = TDepictionTemplate(
                RenderStyleProviderDescriptor = ~/DefaultRenderStyleProvider
                Selector = WarGameShowRoomDepictionSelector
                DepictionAlternatives = [
                    TDepictionDescriptor( SelectorId = 'high' MeshDescriptor = Modele_Letov_S28_CZ_train ),
                    TDepictionDescriptor( SelectorId = 'none' )
                ]
            )
        )
    ]
    + DepictionSubDepiction_Pilote_FR
    + DepictionSubDepiction_Pilote2_FR
    )
)
