    RenderStyleProviderDescriptor = ~/DefaultRenderStyleProvider
    Selector = CommonInfantryDepictionSelector
    DepictionAlternatives = [
        TDepictionDescriptor( SelectorId = 'high' MeshDescriptor = <MeshDescriptorHigh> ),
        TDepictionDescriptor( SelectorId = 'mid'  MeshDescriptor = <MeshDescriptorMid> DisabledOperators = ['ikchain1', 'skeletal']),
        TDepictionDescriptor( SelectorId = 'low'  RequireOthersSkeleton = true DisabledOperators = ['ikchain1', 'skeletal']),
        TDepictionDescriptor( SelectorId = 'none' RequireOthersSkeleton = true  DisabledOperators = ['ikchain1', 'skeletal']),
    ]

    Operators = [
        Template_DepictionOperator_SkeletalAnimation
        (
            AnimationTree = InfanterieTreeTireurDebout()
        ),
        TCosmeticIKChainDesc( OperatorId = 'ikchain1'
                              TargetConstraints = [
                                TCosmeticIKTargetConstraintDesc( Target = <LeftHandTarget> Anchor = 'Bip01 L Hand' ChainLength = 3),
                                TCosmeticIKTargetConstraintDesc( Target = <RightHandTarget> Anchor = 'Bip01 R Hand' ChainLength = 3)
                              ]
                              )
    ]
)
