SubDepictionTowedUnit is TSubDepiction
(
    Anchor = "TowedUnit"
    Depiction = TDepictionTemplate
    (
        RenderStyleProviderDescriptor = ~/DefaultRenderStyleProvider
        Selector = ~/TowedUnitSubDepictionSelector
        DepictionAlternativeOverrideName = "TowedUnitOverride"
        ReplaceableOperatorName = "TowedUnitCarriableOperator"
        DepictionAlternatives = [
            TDepictionDescriptor( SelectorId = 'high'        RequireOthersSkeleton = true),
            TDepictionDescriptor( SelectorId = 'mid'         RequireOthersSkeleton = true),
            TDepictionDescriptor( SelectorId = 'low'         RequireOthersSkeleton = true),
            TDepictionDescriptor( SelectorId = 'none'        RequireOthersSkeleton = true),
        ]
        Operators =
        [TCosmeticSetToPoseOperatorDesc()] +
        [TCosmeticWheelsOperatorDesc( OperatorId = 'Wheels' RotateDirection = false MoveFromSuspension = true ReverseRotation = true )] +
        [TCosmeticCarriableOperatorDesc( OperatorId = 'biped')] +
        [
            TCosmeticTowedUnitOperatorDesc
            (
                OperatorId = 'TowedUnitOperator'
                RootNode = 'Chassis'
                AttachmentPointNode = 'remorque_2'
                LeftWheelNode = 'Roue_Gauche_01'
                RightWheelNode = 'Roue_Droite_01'
                AdditionalWheelNodes = [ 'Roue_G2', 'Roue_D2', 'Roue_02' ]
                WorldFloorProxy = $/M3D/Scene/WorldFloorForIAProxy
            ),
        ]

        // Comme toutes les DepictionAlternatives sont vides (puisque on va les remplir avec les alternatives de l'unité transportée),
        // on oublie le "cache d'utilité" des operators, sinon ce cache va désactiver certains operators comme celui des roues (TCosmeticWheelsOperatorDesc),
        // puisqu'il ne va voir aucune roue dans les DepictionAlternatives vide et va donc penser que l'operator des roues ne servira à rien.
        DisableUsefulnessOperatorsCache = true
    )
)

