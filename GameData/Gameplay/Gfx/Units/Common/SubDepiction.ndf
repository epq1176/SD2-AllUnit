template
InfanterieTreePilote[] is DepictionAnimationTreeNode
(
    IAStates = InfanterieStateAll
    IAOldStates = InfanterieStateAll
    DefaultAnimationStates = InfanterieStateStand
    UsedSlots = InfantryAnimationAllSlots
    Animations = [ TemplateAnimationTopNoPath(Animation='GameData:Assets\3D\Units\Common\Driver\Pilot_Stand.Ase2NdfBin' Mask=AnimationMask_Full_Driver),
                   TemplateAnimationBottomNoPath(Animation='GameData:Assets\3D\Units\Common\Driver\Pilot_Stand.Ase2NdfBin' Mask=AnimationMask_Full_Driver) ]
)

template TemplateDepictionPilote
[
    MeshDescriptorHigh,
    MeshDescriptorMid,
]
is TDepictionTemplate
(
    RenderStyleProviderDescriptor = ~/DefaultRenderStyleProvider
    Selector = CommonInfantryDepictionSelector
    DepictionAlternatives = [
        TDepictionDescriptor( SelectorId = 'high' MeshDescriptor = <MeshDescriptorHigh> ),
        TDepictionDescriptor( SelectorId = 'mid'  MeshDescriptor = <MeshDescriptorMid> ),
        TDepictionDescriptor( SelectorId = 'low'  RequireOthersSkeleton = true),
        TDepictionDescriptor( SelectorId = 'none' RequireOthersSkeleton = true),
    ]

    Operators = [
        Template_DepictionOperator_SkeletalAnimation
        (
            AnimationTree = InfanterieTreePilote()
        )
    ]
)

DepictionPilote_US   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_US MeshDescriptorMid = MeshDescriptor_Pilote_US_LOW )
DepictionPilote_US_Red_Tails   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_US_Red_Tails MeshDescriptorMid = MeshDescriptor_Pilote_US_LOW )
DepictionPilote_FR   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_FR MeshDescriptorMid = MeshDescriptor_Pilote_FR_LOW )
DepictionPilote_GER   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_GER MeshDescriptorMid = MeshDescriptor_Pilote_GER_LOW )
DepictionPilote_UK   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_UK MeshDescriptorMid = MeshDescriptor_Pilote_UK_LOW )
DepictionPilote_SOV   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_SOV MeshDescriptorMid = MeshDescriptor_Pilote_SOV_LOW )
DepictionPilote_FIN   is TemplateDepictionPilote( MeshDescriptorHigh = MeshDescriptor_Pilote_FIN MeshDescriptorMid = MeshDescriptor_Pilote_FIN_LOW )

SubDepictionPilote_US   is TSubDepiction( Anchor = 'Pilot' Depiction = DepictionPilote_US )
SubDepictionPilote_US_Red_Tails   is TSubDepiction( Anchor = 'Pilot' Depiction = DepictionPilote_US_Red_Tails )
