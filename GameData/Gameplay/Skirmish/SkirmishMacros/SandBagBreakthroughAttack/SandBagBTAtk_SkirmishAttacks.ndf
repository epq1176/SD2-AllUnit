    PoolModelList = [
        ~/SandBag_BTAtk_Pool_Main_Attack,
    ]

    TaggedPoolModelList = [
        (["AttackMain"], [~/SandBag_BTAtk_Pool_Main_Attack]),
    ]

    ParametresDeMission =
    ~/SandBag_BTAtk_Default_Attack_Settings
    +
    ~/SandBag_BTAtk_Blitzkrieg_Attack_Settings
    +
    ~/SandBag_BTAtk_AvoidFillerCorridors_Attack_Settings
    +
    MAP
    [
        (
            ESettingType/Coordination,
            TCoordinationSettingHolder(CoordinationType = ESupportingStatus/NeedSupport)
        ),
    ]
)
