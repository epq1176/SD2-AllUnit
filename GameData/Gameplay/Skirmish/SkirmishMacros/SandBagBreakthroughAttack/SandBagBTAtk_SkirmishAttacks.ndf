        (["AttackMBT"], [~/SandBag_BTAtk_Pool_Attack_MBT]),
    ]

    ParametresDeMission =
    ~/SandBag_BTAtk_Default_Attack_Settings
    +
    ~/SandBag_BTAtk_Blitzkrieg_Attack_Settings
    +
    ~/SandBag_BTAtk_AvoidFillerCorridors_Attack_Settings
