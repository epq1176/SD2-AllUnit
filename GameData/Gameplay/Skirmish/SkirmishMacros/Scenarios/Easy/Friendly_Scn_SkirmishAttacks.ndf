private template Friendly_Scn_Template_Attack
[
    MinimumResourceForLaunch:int = 0,
    RatioMinimumPourAttaquer:float = 1.0,
    OrderedZoneWeightList = [1, 2],
]
is TMacroActionDescriptor_Attack_Specific
(
    GenerationType = TGenerationType_FixedSize
    NumberOfMacro = 1
    MinimumResourceForLaunch = <MinimumResourceForLaunch>
    UseAntiStackingForUnits = true
    CompterLesUnitesEnZoneExtendedDansLaMenace = true
    UtiliserMenaceLaPlusProcheAuLieuDeLaPlusFaible = true
    RatioMinimumPourAttaquer = <RatioMinimumPourAttaquer>
    DisableReinforcement = false
    IgnoreSafePath = false
    MargeDeRegroupementDepuisLaLigneDeFront = 1
    OrderedZoneWeightList = <OrderedZoneWeightList>

    LaunchASAP_RatioMin = 0.1
    ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]

    PoolModelList =
    [
        Friendly_Scn_Pool_Default_Attack_1,
        Friendly_Scn_Pool_Default_Attack_2,
        Friendly_Scn_Pool_Default_Attack_3,
    ]

    TaggedPoolModelList =
    [
        (["AttackInfantry"], [ Friendly_Scn_Pool_Infantry_Attack_1, Friendly_Scn_Pool_Infantry_Attack_2, Friendly_Scn_Pool_Infantry_Attack_3 ]),
        (["AttackCombined"], [ Friendly_Scn_Pool_Combined_Attack_1, Friendly_Scn_Pool_Combined_Attack_2, Friendly_Scn_Pool_Combined_Attack_3 ]),
        (["AttackTank"], [ Friendly_Scn_Pool_Tank_Attack_1, Friendly_Scn_Pool_Tank_Attack_2, Friendly_Scn_Pool_Tank_Attack_3 ]),
    ]

    ParametresDeMission =
    ~/Friendly_Scn_Default_Attack_Settings
    +
    ~/Friendly_Scn_Blitzkrieg_Attack_Settings
)
