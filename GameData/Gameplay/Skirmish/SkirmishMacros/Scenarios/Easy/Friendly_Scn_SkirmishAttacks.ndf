private template Friendly_Scn_Template_Attack_MBT
[
    MinimumResourceForLaunch:int = 0,
    IsSupportingOtherAttacks:bool = false,
]
is TMacroActionDescriptor_CorridorDynamicDefense_Specific
(
    GenerationType = TGenerationType_FixedSize
    NumberOfMacro = 1
    MinimumResourceForLaunch = <MinimumResourceForLaunch>
    IgnoreSafePath = false
    DisableReinforcement = false
    LaunchASAP_RatioMin = 0.1
    ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]

    IgnoreVisionForDefensePosition = false
    IgnoreEnemyDirectionForDefensePosition = false
    IgnoreDefensePositionSafety = true

    TaggedPoolModelList = [
        (["AttackMBT"], [~/Friendly_Scn_Pool_Attack_MBT]),
    ]

    ParametresDeMission =
    ~/Friendly_Scn_Default_Attack_Settings
    +
    ~/Friendly_Scn_Blitzkrieg_Attack_Settings
    +
    ~/Friendly_ScnAtk_AvoidFillerCorridors_Attack_Settings
    +
    (
        <IsSupportingOtherAttacks> == false ? MAP[]:
        MAP
        [
            (
                ESettingType/Coordination,
                TCoordinationSettingHolder(CoordinationType = ESupportingStatus/IsSupport)
            ),
        ]
    )
)
