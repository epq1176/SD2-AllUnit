private template ScnAtk_Template_ZoneAttack
[
    MinimumResourceForLaunch:int = 0,
    RatioMinimumPourAttaquer:float = 1.0,
    RatioPourAttaquerPerduAChaqueTick:float = 0.0005,
    OrderedZoneWeightList = [3, 2, 1],
]
is TMacroActionDescriptor_Attack_Specific
(
    GenerationType = TGenerationType_FixedSize
    NumberOfMacro = 1
    MinimumResourceForLaunch = <MinimumResourceForLaunch>
    UseAntiStackingForUnits = true
    CompterLesUnitesEnZoneExtendedDansLaMenace = true
    UtiliserMenaceLaPlusProcheAuLieuDeLaPlusFaible = true
    RatioMinimumPourAttaquer = <RatioMinimumPourAttaquer>
    RatioPourAttaquerPerduAChaqueTick = <RatioPourAttaquerPerduAChaqueTick>
    DisableReinforcement = false
    IgnoreSafePath = false
    MargeDeRegroupementDepuisLaLigneDeFront = 1
    OrderedZoneWeightList = <OrderedZoneWeightList>

    LaunchASAP_RatioMin = 0.01
    ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]

    PoolModelList =
    [
        ScnAtk_Pool_Default_ZoneAttack_1,
        ScnAtk_Pool_Default_ZoneAttack_2,
        ScnAtk_Pool_Default_ZoneAttack_3,
    ]

    TaggedPoolModelList =
    [
        (["AttackInfantry"], [ ScnAtk_Pool_Infantry_ZoneAttack_1, ScnAtk_Pool_Infantry_ZoneAttack_2, ScnAtk_Pool_Infantry_ZoneAttack_3 ]),
        (["AttackCombined"], [ ScnAtk_Pool_Combined_ZoneAttack_1, ScnAtk_Pool_Combined_ZoneAttack_2, ScnAtk_Pool_Combined_ZoneAttack_3 ]),
        (["AttackTank"], [ ScnAtk_Pool_Tank_ZoneAttack_1, ScnAtk_Pool_Tank_ZoneAttack_2, ScnAtk_Pool_Tank_ZoneAttack_3 ]),
    ]

    ParametresDeMission =
    ~/ScnAtk_Default_Attack_Settings
    +
    ~/ScnAtk_Blitzkrieg_Attack_Settings
)
