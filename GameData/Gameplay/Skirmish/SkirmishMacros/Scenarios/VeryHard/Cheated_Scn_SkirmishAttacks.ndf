        (["AttackMBT"], [~/Cheated_Scn_Pool_Attack_MBT]),
    ]

    ParametresDeMission =
    ~/Cheated_Scn_Default_Attack_Settings
    +
    ~/Cheated_Scn_Blitzkrieg_Attack_Settings
    +
    ~/Cheated_ScnAtk_AvoidFillerCorridors_Attack_Settings
    +
    (
        <IsSupportingOtherAttacks> == false ? MAP[]:
        MAP
        [
            (
                ESettingType/Coordination,
                TCoordinationSettingHolder(CoordinationType = ESupportingStatus/IsSupport)
            ),
        ]
    )
)
