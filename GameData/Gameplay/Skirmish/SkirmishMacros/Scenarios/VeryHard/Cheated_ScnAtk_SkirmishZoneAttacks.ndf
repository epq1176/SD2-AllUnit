    IgnoreSafePath = false
    MargeDeRegroupementDepuisLaLigneDeFront = 1
    OrderedZoneWeightList = <OrderedZoneWeightList>

    LaunchASAP_RatioMin = 0.01
    ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]

    PoolModelList =
    [
        Cheated_ScnAtk_Pool_Default_ZoneAttack_1,
        Cheated_ScnAtk_Pool_Default_ZoneAttack_2,
        Cheated_ScnAtk_Pool_Default_ZoneAttack_3,
    ]

    TaggedPoolModelList =
    [
        (["AttackInfantry"], [ Cheated_ScnAtk_Pool_Infantry_ZoneAttack_1, Cheated_ScnAtk_Pool_Infantry_ZoneAttack_2, Cheated_ScnAtk_Pool_Infantry_ZoneAttack_3 ]),
