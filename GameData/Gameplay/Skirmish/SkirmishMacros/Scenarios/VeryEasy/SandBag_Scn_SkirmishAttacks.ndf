    NumberOfMacro = 1
    CompterLesUnitesEnZoneExtendedDansLaMenace = true
    RatioMinimumPourAttaquer = 1.2
    RatioPourAttaquerPerduAChaqueTick = 0.00084 // Perte de 0.1 de ratio toutes les 2 minutes
    DisableReinforcement = false
    IgnoreSafePath = false
