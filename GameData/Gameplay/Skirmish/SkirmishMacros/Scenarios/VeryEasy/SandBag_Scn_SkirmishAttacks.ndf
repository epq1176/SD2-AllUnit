private template SandBag_Scn_Template_Attack
[
    MinimumResourceForLaunch:int = 0,
    RatioMinimumPourAttaquer:float = 1.0,
    OrderedZoneWeightList = [1, 2],
]
is TMacroActionDescriptor_Attack_Specific
(
    GenerationType = TGenerationType_FixedSize
    NumberOfMacro = 1
    MinimumResourceForLaunch = <MinimumResourceForLaunch>
    UseAntiStackingForUnits = true
    CompterLesUnitesEnZoneExtendedDansLaMenace = true
    UtiliserMenaceLaPlusProcheAuLieuDeLaPlusFaible = true
    RatioMinimumPourAttaquer = <RatioMinimumPourAttaquer>
    DisableReinforcement = false
    IgnoreSafePath = false
    MargeDeRegroupementDepuisLaLigneDeFront = 1
    OrderedZoneWeightList = <OrderedZoneWeightList>

    LaunchASAP_RatioMin = 0.1
    ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]

    PoolModelList =
    [
        SandBag_Scn_Pool_Default_Attack_1,
        SandBag_Scn_Pool_Default_Attack_2,
        SandBag_Scn_Pool_Default_Attack_3,
    ]

    TaggedPoolModelList =
    [
        (["AttackInfantry"], [ SandBag_Scn_Pool_Infantry_Attack_1, SandBag_Scn_Pool_Infantry_Attack_2, SandBag_Scn_Pool_Infantry_Attack_3 ]),
        (["AttackCombined"], [ SandBag_Scn_Pool_Combined_Attack_1, SandBag_Scn_Pool_Combined_Attack_2, SandBag_Scn_Pool_Combined_Attack_3 ]),
        (["AttackTank"], [ SandBag_Scn_Pool_Tank_Attack_1, SandBag_Scn_Pool_Tank_Attack_2, SandBag_Scn_Pool_Tank_Attack_3 ]),
    ]

    ParametresDeMission =
    ~/SandBag_Scn_Default_Attack_Settings
    +
    ~/SandBag_Scn_Blitzkrieg_Attack_Settings
)
