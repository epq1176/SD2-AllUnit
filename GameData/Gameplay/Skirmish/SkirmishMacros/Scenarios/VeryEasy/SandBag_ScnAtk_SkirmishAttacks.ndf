private template SandBag_ScnAtk_Template_Main_Attack
[
    MinimumResourceForLaunch:int = 0,
]
is TMacroActionDescriptor_CorridorAttack_Specific
(
    GenerationType = TGenerationType_FixedSize
    MinimumResourceForLaunch = <MinimumResourceForLaunch>
    NumberOfMacro = 1
    CompterLesUnitesEnZoneExtendedDansLaMenace = true
    RatioMinimumPourAttaquer = 1.2
    RatioPourAttaquerPerduAChaqueTick = 0.00084 // Perte de 0.1 de ratio toutes les 2 minutes
    DisableReinforcement = false
    StayInCorridorIfp = true
    IgnoreSafePath = false
    MargeDeRegroupementDepuisLaLigneDeFront = 1
    ActiveComportementEnBoutDeCouloir = true
    PrioritizeCorridorsForTaggedPoolModels = true

    LaunchASAP_RatioMin = 0.01
    ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]

    PoolModelList = [
        ~/SandBag_ScnAtk_Pool_Main_Attack,
    ]

    TaggedPoolModelList = [
        (["AttackMain"], [~/SandBag_ScnAtk_Pool_Main_Attack]),
    ]

    ParametresDeMission =
    ~/SandBag_ScnAtk_Default_Attack_Settings
    +
    ~/SandBag_ScnAtk_Blitzkrieg_Attack_Settings
    +
    ~/SandBag_ScnAtk_AvoidFillerCorridors_Attack_Settings
    +
    MAP
    [
        (
            ESettingType/Coordination,
            TCoordinationSettingHolder(CoordinationType = ESupportingStatus/NeedSupport)
        ),
    ]
)
