template DeckCreatorEditCategory
[
    ElementName : string = "",

    HasTitle : bool = false,
    TitleToken : string = "",

    FirstMargin : TRTTILength = TRTTILength( Magnifiable = 5.0 ),

    Content : TBUCKContainerDescriptor,
]
is BUCKListDescriptor
(
    ElementName = <ElementName>
    ComponentFrame = TUIFramePropertyRTTI
    (
        RelativeWidthHeight = [0.0, 0.0]
    )

    Axis = ~/ListAxis/Horizontal
    FirstMargin = <FirstMargin>
    InterItemMargin = TRTTILength( Magnifiable = 0.0 )
    LastMargin = TRTTILength( Magnifiable = 5.0 )

    UniformDrawer = $/UserInterface/UIUniformDrawer
    HasBorder = false

    BreadthComputationMode = ~/BreadthComputationMode/ComputeBreadthFromLargestChild

    Elements = [] +
    (<HasTitle> ?
    [
        BUCKListElementDescriptor
        (
            ExtendWeight = 0.5
            ComponentDescriptor = BUCKTextDescriptor
            (
                ComponentFrame = TUIFramePropertyRTTI()
                ParagraphStyle = TParagraphStyleRTTI
                (
                    Alignment = UIText_Left
                    VerticalAlignment = UIText_VerticalCenter
                    InterLine = 0
                )

                TextStyle = "Default"
                HorizontalFitStyle = ~/FitStyle/FitToContent
                VerticalFitStyle = ~/FitStyle/FitToContent
                TextPadding = TRTTILength4(Magnifiable = [0.0, 5.0, 0.0, 5.0])
                TypefaceToken = "Main"
                BigLineAction = ~/BigLineAction/CutByDots

                UniformDrawer = $/UserInterface/UIUniformDrawer

                TextDico = ~/LocalisationConstantes/dico_interface_ingame
                TextToken  = <TitleToken>
                TextColor = "DeckEditScreen/Edition/Category/Title"
                TextSize = "DeckEditScreen/Edition/Category/Title"
            )
        )
    ] : [] ) +

    [
        BUCKListElementDescriptor
        (
            ExtendWeight = 0.5
            ComponentDescriptor = <Content>
        )
    ]
)

